{
  "name": "@graphql-toolkit/url-loader",
  "version": "0.9.1",
  "description": "A set of utils for faster development of GraphQL tools",
  "repository": "git@github.com:ardatan/graphql-toolkit.git",
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "bob",
    "prepack": "bob-update-version",
    "test": "jest --passWithNoTests --passWithNoTests --no-watchman --config ../../../jest.config.js"
  },
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "devDependencies": {
    "@ardatan/bob": "0.2.6",
    "@types/express": "4.17.2",
    "@types/jest": "24.0.25",
    "@types/supertest": "2.0.8",
    "apollo-server-express": "2.9.16",
    "express": "4.17.1",
    "graphql": "14.5.8",
    "graphql-tools": "4.0.6",
    "jest": "24.9.0",
    "supertest": "4.0.2",
    "ts-jest": "24.3.0",
    "typescript": "3.7.4"
  },
  "dependencies": {
    "@graphql-toolkit/common": "0.9.1",
    "cross-fetch": "3.0.4",
    "tslib": "1.10.0",
    "valid-url": "1.0.9"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
