{
  "name": "@graphql-toolkit/url-loader",
  "version": "0.9.12",
  "description": "A set of utils for faster development of GraphQL tools",
  "repository": "git@github.com:ardatan/graphql-toolkit.git",
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "bob",
    "prepack": "bob-update-version",
    "test": "jest --passWithNoTests --passWithNoTests --no-watchman --config ../../../jest.config.js"
  },
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "devDependencies": {
    "@ardatan/bob": "0.2.7",
    "@types/express": "4.17.4",
    "@types/jest": "25.1.4",
    "@types/supertest": "2.0.8",
    "apollo-server-express": "2.11.0",
    "express": "4.17.1",
    "graphql": "14.6.0",
    "jest": "25.2.6",
    "supertest": "4.0.2",
    "ts-jest": "25.3.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@graphql-toolkit/common": "0.9.12",
    "cross-fetch": "3.0.4",
    "graphql-tools-fork": "9.0.1",
    "tslib": "1.11.1",
    "valid-url": "1.0.9"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
