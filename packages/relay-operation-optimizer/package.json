{
  "name": "@graphql-toolkit/relay-operation-optimizer",
  "version": "0.9.1",
  "description": "Package for optimizing your GraphQL operations relay style.",
  "author": {
    "name": "Laurin Quast",
    "email": "laurinquast@googlemail.com",
    "url": "https://github.com/n1ru4l"
  },
  "license": "MIT",
  "repository": "git@github.com:ardatan/graphql-toolkit.git",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "bob",
    "prepack": "bob-update-version",
    "test": "jest --passWithNoTests --no-watchman --config ../../jest.config.js"
  },
  "keywords": [
    "graphql",
    "codegen",
    "graphql-codegen",
    "plugin",
    "relay"
  ],
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "dependencies": {
    "@graphql-toolkit/common": "0.9.1",
    "relay-compiler": "8.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.9.1",
    "@types/relay-compiler": "7.0.2",
    "@ardatan/bob": "0.2.7",
    "graphql": "14.5.8",
    "jest": "25.1.0",
    "ts-jest": "25.0.0",
    "typescript": "3.7.5"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
